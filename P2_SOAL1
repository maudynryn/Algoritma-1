#include <iostream>
#include <string>
using namespace std;

int main()
{
	string nama;
	nama = "Abdullah";

	int usia;
	usia = 64;

	float tinggi;
	tinggi = 173.64;

	string hobby;
	hobby = "main bola, nulis, ngoding";

	cout <<"isi nama = "<<nama<<'\n';
	cout <<"isi usia = "<<usia<<'\n'; 
	cout <<"isi tinggi = "<<tinggi<<'\n';
	cout <<"isi hobby = "<<hobby<<'\n';
	return 0;
}
